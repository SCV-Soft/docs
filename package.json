{
  "name": "@hirosystems/docs",
  "version": "0.1.0",
  "private": true,
  "prettier": "@stacks/prettier-config",
  "scripts": {
    "build": "yarn update:cli-ref && docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "dev": "docusaurus start",
    "docusaurus": "docusaurus",
    "postinstall": "patch-package",
    "serve": "docusaurus serve",
    "spellcheck": "vale --config='.vale.ini' docs/**",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc",
    "update:cli": "yarn upgrade @stacks/cli@latest --dev",
    "update:cli-ref": "stx docs > src/_data/cli-reference.json && node ./update-cli-ref.js ./src/_data/cli-reference.json -o ./docs/references/",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "dependencies": {
    "@cmfcmf/docusaurus-search-local": "^0.11.0",
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@docusaurus/theme-live-codeblock": "^2.4.1",
    "@mdx-js/react": "^1.6.21",
    "@stacks/auth": "^6.5.4",
    "@stacks/blockchain-api-client": "^7.2.0",
    "@stacks/common": "^6.5.2",
    "@stacks/connect": "^7.3.1",
    "@stacks/encryption": "^6.5.4",
    "@stacks/network": "^6.5.4",
    "@stacks/profile": "^6.5.4",
    "@stacks/stacking": "^6.5.4",
    "@stacks/storage": "^6.5.4",
    "@stacks/transactions": "^6.5.4",
    "@stacks/wallet-sdk": "^6.5.4",
    "@svgr/webpack": "^6.3.1",
    "buffer": "^6.0.3",
    "buffer-browserify": "^0.2.5",
    "clsx": "^1.1.1",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "docusaurus-plugin-remote-content": "^3.1.0",
    "docusaurus-plugin-segment": "^1.0.3",
    "file-loader": "^6.2.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redocusaurus": "^1.6.2",
    "stream-browserify": "^3.0.0",
    "url-loader": "^4.1.1",
    "util": "^0.12.5",
    "webpack-node-externals": "^3.0.0",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@stacks/cli": "^6.0.2",
    "@stacks/prettier-config": "^0.0.10",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16 <19"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
